	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Debug",
			"skipFiles": ["<node_internals>/**"],
			"attachSimplePort": 5878,
			"timeout": 20000,
			"runtimeArgs": [
				"--inspect-brk=5878",
				"--no-cached-data",
			],
			"windows": {
				"runtimeExecutable": "${workspaceFolder}/build/debug.bat"
			},
			"program": "${workspaceFolder}\\out\\main.js"
		}
	]
}
